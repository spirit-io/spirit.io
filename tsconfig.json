{
    "compilerOptions": {
        "target": "es2015",
        "module": "commonjs",
        "noEmitOnError": true,
        "noImplicitAny": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "sourceMap": true,
        "inlineSources": true,
        "outDir": "dist",
        "moduleResolution": "node",
        "strictNullChecks": false
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}